{
  "image": "ucsbplse/cs292c:8.18.0",
  "customizations": {
    "vscode": {
      "extensions": [
        "maximedenes.vscoq"
      ],
      "settings": {
        "coqtop.binPath": "/home/coq/.opam/default/bin",
        "files.exclude": {
          "**/*.aux": true,
          "**/*.glob": true,
          "**/*.vo": true,
          "**/*.vos": true,
          "**/*.vok": true,
          "**/*.html": true,
          "**/.*.report": true,
          "**/.*.cache": true
        },
        "coq.loadCoqProject": true,
        "coq.coqProjectRoot": ".",
        "[coq]": {
          "editor.tabSize": 2,
          "editor.insertSpaces": true
        }
      }
    }
  }
}